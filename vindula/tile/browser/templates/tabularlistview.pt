<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="context/main_template/macros/master"
      i18n:domain="vindula.tile">

    <head>
      <metal:js fill-slot="javascript_head_slot">

        <script type="text/javascript" src=""
                tal:attributes="src string:${context/absolute_url|nothing}/++resource++vindula.tile/js/ajax_boll_batch.js"></script>
      </metal:js>

    </head>
    <body>
        <metal:main fill-slot="content-core">
            <metal:macro define-macro="page">

                <metal:macro define-macro="body"
                             tal:define="view_macro nocall:context/@@tabularlist-view;">
                    <div id="tile-tabular-list">
                        <metal:block metal:use-macro="context/macro_filter_file/macros/tabular"/>
                        <div id="itens">
                            <tal:def condition="structures"
                                     define="portal_type context/getObject_type;
                                             structures nocall:view_macro/getStructure|nothing;">
                                <tal:def define="structures python:structures().UID() or None;
                                                  fields python:[{'label': 'Título', 'attribute': 'Title'},
                                                                 {'label': 'Criação', 'attribute': 'getRawCreation_date'},];">
                                    <metal:block metal:use-macro="context/macro_tabular_file/macros/view" />
                                </tal:def>
                            </tal:def>
                        </div>
                    </div>
                </metal:macro>
                
            </metal:macro>
        </metal:main>
    </body>
</html>